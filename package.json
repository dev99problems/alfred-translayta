    "test:cov": "yarn test --collectCoverage=true --collectCoverageFrom=src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh@github.com:dev99problems/alfred-translayta.git"
  },
  "author": {
    "name": "Eugene Chulkov",
    "email": "genechulkov@gmail.com",
    "url": "https://twitter.com/dev99problems"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "alfred",
    "alfy",
    "translate",
    "alfred-translate",
    "alfred-translayta",
    "workflow",
    "mac",
    "macos",
    "osx"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dev99problems/alfred-translayta/issues"
  },
  "homepage": "https://github.com/dev99problems/alfred-translayta#readme",
  "dependencies": {
    "@vitalets/google-translate-api": "^5.0.0",
    "alfy": "0.11.1",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "alfy-test": "^0.4.0",
    "husky": "^3.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
